seg data:

byte

word
dword
qword

seg code:

func main:
    mov     5, r1
    mov     8, r2
    sub     r1, r2, r3
    if r3 == 3:
        call    print("1")
    else if r3 == -3:
        call    print("2")
    else:
        call print("3")
    ret

extern func print: reg
