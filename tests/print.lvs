const SYS_WRITE: int = 1;

function print(str: string = "") {
    var str_len: int = s.length;
    asm `
        mov     rdx, str_len        ; put length in rdx
        mov     rsi, str            ; string1 to source index
        mov     rax, SYS_WRITE      ; set write to command
        mov     rdi, rax            ; set destination index to rax (stdout)
        syscall
    `;
}