function main(): int {
    i: int = 39;
    j: int = 3;
    k: int = 10;
    asm {
        mov eax, i
        mov ebx, j
        add eax, ebx        // same as i = i + j or i += j
        mov ebx, k
        mult eax, ebx       // same as i = i * k or i *= k
        mov i, eax
    }
    test();
    return i;
}

function test(): void {
    x: int = 10;
    asm {
        mov eax, &x         // store address of x in eax
    }
}