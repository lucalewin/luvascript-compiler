function min(a: int, b: int): int {
	if (a < b) return a;
	return b;
}

function max(a: int, b: int): int {
	if (a > b) return a;
	return b;
}

function pow(a: int, b: int): int {
	if (b == 0) return 1;
	if (b == 1) return a;
	if (b % 2 == 0) return pow(a * a, b / 2);
	return a * pow(a * a, b / 2);
}
